{
    "crons": [
        {
            "path": "/api/sync-sheets",
            "schedule": "0 * * * *"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-store, max-age=0, must-revalidate"
                }
            ]
        }
    ]
}